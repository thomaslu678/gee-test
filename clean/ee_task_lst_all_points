      "source": [
        "landsat_collections = {\n",
        "    \"L4\": (\"LANDSAT/LT04/C02/T1_L2\", \"ST_B6\"),\n",
        "    \"L5\": (\"LANDSAT/LT05/C02/T1_L2\", \"ST_B6\"),\n",
        "    \"L7\": (\"LANDSAT/LE07/C02/T1_L2\", \"ST_B6\"),\n",
        "    \"L8\": (\"LANDSAT/LC08/C02/T1_L2\", \"ST_B10\"),\n",
        "    \"L9\": (\"LANDSAT/LC09/C02/T1_L2\", \"ST_B10\"),\n",
        "}"
      ],
      "metadata": {
        "id": "gu-GD1KjzO_v"
      },
      "execution_count": 5,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "# Functions"
      ],
      "metadata": {
        "id": "iGfbXnrVd6zn"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "def mask_qa_pixel(img: ee.Image):\n",
        "    # QA_PIXEL is the name of the fetched file that provides info\n",
        "    # about pixel quality assurance\n",
        "    qa = img.select(\"QA_PIXEL\")\n",
        "\n",
        "    # Fetch bits (0/1) describing if an environmental condition\n",
        "    # is present\n",
        "    cloud        = qa.bitwiseAnd(1 << 3).neq(0)\n",
        "    cloud_shadow = qa.bitwiseAnd(1 << 4).neq(0)\n",
        "    snow         = qa.bitwiseAnd(1 << 5).neq(0)\n",
        "    cirrus       = qa.bitwiseAnd(1 << 2).neq(0)\n",
        "\n",
        "    mask = (\n",
        "        # Or condition for all bad conditions\n",
        "        cloud\n",
        "        .Or(cloud_shadow)\n",
        "        .Or(snow)\n",
        "        .Or(cirrus)\n",
        "\n",
        "        # Not flips the conditions so any remaining pixels\n",
        "        # have none of the conditions, and are \"good\" quality.\n",
        "        .Not()\n",
        "    )\n",
        "\n",
        "    # Apply the mask to the input image\n",
        "    return img.updateMask(mask)"
      ],
      "metadata": {
        "id": "27vz9ubfLlhZ"
      },
      "execution_count": 6,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def scale_lst(img, band):\n",
        "    lst = (\n",
        "\n",
        "        # Select the correct band, either ST_B6 or ST_B10\n",
        "        img.select(band)\n",
        "        .multiply(0.00341802)\n",
        "        .add(149.0)\n",
        "        .rename(\"LST_K\")\n",
        "    )\n",
        "\n",
        "    # Add a new band to the original image, with the scaled LST\n",
        "    return img.addBands(lst)"
      ],
      "metadata": {
        "id": "WnU2sb1CLnc1"
      },
      "execution_count": 7,
      "outputs": []
    },
    {
      "cell_type": "markdown",
      "source": [
        "# Loop over all points"
      ],
      "metadata": {
        "id": "pvQMlGFo17Zy"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "input_df = pd.read_csv('/content/sample_data/points.csv')"
      ],
      "metadata": {
        "id": "ZqZzFdO1POJZ"
      },
      "execution_count": 10,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def df_to_points_fc(df):\n",
        "    feats = []\n",
        "    for _, r in df.iterrows():\n",
        "        geom = ee.Geometry.Point(float(r[\"long\"]), float(r[\"lat\"]))\n",
        "        feats.append(\n",
        "            ee.Feature(geom, {\n",
        "                \"lat\": float(r[\"lat\"]),\n",
        "                \"long\": float(r[\"long\"]),\n",
        "                \"distance\": float(r[\"distance\"])\n",
        "            })\n",
        "        )\n",
        "    return ee.FeatureCollection(feats)\n",
        "\n",
        "points_fc = df_to_points_fc(input_df)"
      ],
      "metadata": {
        "id": "UQGuSeJ0p9cZ"
      },
      "execution_count": 11,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "city_geom = points_fc.geometry().convexHull(maxError=1)"
      ],
      "metadata": {
        "id": "aDpIna7gsR9M"
      },
      "execution_count": 13,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def sample_landsat_collection(collection_id, band, mission, city_geom, points_fc):\n",
        "    col = (\n",
        "        ee.ImageCollection(collection_id)\n",
        "        .filterBounds(city_geom)     # ONE image per city per overpass\n",
        "        .map(mask_qa_pixel)\n",
        "        .map(lambda img: scale_lst(img, band))\n",
        "    )\n",
        "\n",
        "    def per_image(img):\n",
        "        samples = img.select(\"LST_K\").sampleRegions(\n",
        "            collection=points_fc,\n",
        "            geometries=False,\n",
        "            scale=None    # native projection â†’ avoids EPSG errors\n",
        "        )\n",
        "\n",
        "        return samples.map(lambda f: f.set({\n",
        "            \"mission\": mission,\n",
        "            \"image_id\": img.id(),\n",
        "            \"time\": ee.Date(img.get(\"system:time_start\"))\n",
        "                        .format(\"YYYY-MM-dd HH:mm:ss\")\n",
        "        }))\n",
        "\n",
        "    return col.map(per_image).flatten()"
      ],
      "metadata": {
        "id": "-mHDTHSQro2t"
      },
      "execution_count": 14,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "collections = []\n",
        "\n",
        "for mission, (collection_id, band) in landsat_collections.items():\n",
        "    fc = sample_landsat_collection(\n",
        "        collection_id, band, mission, city_geom, points_fc\n",
        "    )\n",
        "    collections.append(fc)\n",
        "\n",
        "final_fc = ee.FeatureCollection(collections).flatten()"
      ],
      "metadata": {
        "id": "yxW3zORfrum4"
      },
      "execution_count": 15,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "ee.batch.Export.table.toDrive(\n",
        "    collection=final_fc,\n",
        "    description=\"landsat_lst_city_points\",\n",
        "    fileFormat=\"CSV\"\n",
        ").start()"
      ],
      "metadata": {
        "id": "5gCMFWwJsbD3"
      },
      "execution_count": null,
      "outputs": []
    },
